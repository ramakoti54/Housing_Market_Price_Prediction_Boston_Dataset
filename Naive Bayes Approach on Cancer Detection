  Ben_Record28=j+29
  Ben_Record29=j+30
  Ben_Record30=j+31
  for i in range(30):
        feature_name = feature_names[i]

        Mean_Radius_Mal =((dataframe[feature_name][Mal_Record1]+dataframe[feature_name][Mal_Record2]+dataframe[feature_name][Mal_Record3]+dataframe[feature_name][Mal_Record4]+dataframe[feature_name][Mal_Record5]+dataframe[feature_name][Mal_Record6]+dataframe[feature_name][Mal_Record7]+dataframe[feature_name][Mal_Record8]+dataframe[feature_name][Mal_Record9]+dataframe[feature_name][Mal_Record10]+dataframe[feature_name][Mal_Record11]+dataframe[feature_name][Mal_Record12]+dataframe[feature_name][Mal_Record13]+dataframe[feature_name][Mal_Record14]+dataframe[feature_name][Mal_Record15]+dataframe[feature_name][Mal_Record16]+dataframe[feature_name][Mal_Record17]+dataframe[feature_name][Mal_Record18]+dataframe[feature_name][Mal_Record19]+dataframe[feature_name][Mal_Record20])/Mal)
        Std_Radius_Mal = np.sqrt(((dataframe[feature_name][Mal_Record1]-Mean_Radius_Mal)*(dataframe[feature_name][Mal_Record1]-Mean_Radius_Mal)+(dataframe[feature_name][Mal_Record2]-Mean_Radius_Mal)*(dataframe[feature_name][Mal_Record2]-Mean_Radius_Mal)+(dataframe[feature_name][Mal_Record3]-Mean_Radius_Mal)*(dataframe[feature_name][Mal_Record3]-Mean_Radius_Mal)+(dataframe[feature_name][Mal_Record4]-Mean_Radius_Mal)*(dataframe[feature_name][Mal_Record4]-Mean_Radius_Mal)+(dataframe[feature_name][Mal_Record5]-Mean_Radius_Mal)*(dataframe[feature_name][Mal_Record5]-Mean_Radius_Mal)+(dataframe[feature_name][Mal_Record6]-Mean_Radius_Mal)*(dataframe[feature_name][Mal_Record6]-Mean_Radius_Mal)+(dataframe[feature_name][Mal_Record7]-Mean_Radius_Mal)*(dataframe[feature_name][Mal_Record7]-Mean_Radius_Mal)+(dataframe[feature_name][Mal_Record8]-Mean_Radius_Mal)*(dataframe[feature_name][Mal_Record8]-Mean_Radius_Mal)+(dataframe[feature_name][Mal_Record9]-Mean_Radius_Mal)*(dataframe[feature_name][Mal_Record9]-Mean_Radius_Mal)+(dataframe[feature_name][Mal_Record10]-Mean_Radius_Mal)*(dataframe[feature_name][Mal_Record10]-Mean_Radius_Mal)+(dataframe[feature_name][Mal_Record11]-Mean_Radius_Mal)*(dataframe[feature_name][Mal_Record11]-Mean_Radius_Mal)+(dataframe[feature_name][Mal_Record12]-Mean_Radius_Mal)*(dataframe[feature_name][Mal_Record12]-Mean_Radius_Mal)+(dataframe[feature_name][Mal_Record13]-Mean_Radius_Mal)*(dataframe[feature_name][Mal_Record13]-Mean_Radius_Mal)+(dataframe[feature_name][Mal_Record14]-Mean_Radius_Mal)*(dataframe[feature_name][Mal_Record14]-Mean_Radius_Mal)+(dataframe[feature_name][Mal_Record15]-Mean_Radius_Mal)*(dataframe[feature_name][Mal_Record15]-Mean_Radius_Mal)+(dataframe[feature_name][Mal_Record16]-Mean_Radius_Mal)*(dataframe[feature_name][Mal_Record16]-Mean_Radius_Mal)+(dataframe[feature_name][Mal_Record17]-Mean_Radius_Mal)*(dataframe[feature_name][Mal_Record17]-Mean_Radius_Mal)+(dataframe[feature_name][Mal_Record18]-Mean_Radius_Mal)*(dataframe[feature_name]