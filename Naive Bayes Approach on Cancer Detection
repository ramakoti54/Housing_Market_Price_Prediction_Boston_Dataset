        exponent_function6=np.exp(-((dataframe[feature_name][Ben_Record4]-Mean_Radius_Mal)*(dataframe[feature_name][Ben_Record4]-Mean_Radius_Mal))/(2*Std_Radius_Mal*Std_Radius_Mal))
        Gaussian6 = exponent_function6*(1/((np.sqrt(2*3.141592653589793238))*Std_Radius_Mal)) 
        exponent_function7=np.exp(-((dataframe[feature_name][Ben_Record5]-Mean_Radius_Mal)*(dataframe[feature_name][Ben_Record5]-Mean_Radius_Mal))/(2*Std_Radius_Mal*Std_Radius_Mal))
        Gaussian7 = exponent_function7*(1/((np.sqrt(2*3.141592653589793238))*Std_Radius_Mal)) 
        exponent_function8=np.exp(-((dataframe[feature_name][Ben_Record6]-Mean_Radius_Mal)*(dataframe[feature_name][Ben_Record6]-Mean_Radius_Mal))/(2*Std_Radius_Mal*Std_Radius_Mal))
        Gaussian8 = exponent_function8*(1/((np.sqrt(2*3.141592653589793238))*Std_Radius_Mal)) 
