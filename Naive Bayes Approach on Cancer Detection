        exponent_function46=0
        exponent_function47=0
        exponent_function48=0
        exponent_function49=0
        exponent_function50=0

        exponent_function1=np.exp(-((dataframe[feature_name][Mal_Record1]-Mean_Radius_Mal)*(dataframe[feature_name][Mal_Record1]-Mean_Radius_Mal))/(2*Std_Radius_Mal*Std_Radius_Mal))
        Gaussian1 = exponent_function1*(1/((np.sqrt(2*3.141592653589793238))*Std_Radius_Mal))                                  
        exponent_function2=np.exp(-((dataframe[feature_name][Ben_Record1]-Mean_Radius_Mal)*(dataframe[feature_name][Ben_Record1]-Mean_Radius_Mal))/(2*Std_Radius_Mal*Std_Radius_Mal))
        Gaussian2 = exponent_function2*(1/((np.sqrt(2*3.141592653589793238))*Std_Radius_Mal)) 
