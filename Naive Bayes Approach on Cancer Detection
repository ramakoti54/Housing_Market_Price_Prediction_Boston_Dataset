        exponent_function26=0
        exponent_function27=0
        exponent_function28=0
        exponent_function29=0
        exponent_function30=0
        exponent_function31=0
        exponent_function32=0
        exponent_function33=0
        exponent_function34=0
        exponent_function35=0
        exponent_function36=0
        exponent_function37=0
        exponent_function38=0
        exponent_function39=0
        exponent_function40=0
        exponent_function41=0
        exponent_function42=0
