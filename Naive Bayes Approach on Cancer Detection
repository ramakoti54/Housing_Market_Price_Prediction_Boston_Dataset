""" 
/**** Importing pandas packages  ****/
/**** Importing matplotlib for visualization ****/
/**** Importing Numpy package for numerical functionality ****/
"""
import pandas as pd
import numpy as np
import matplotlib.pyplot as plots

# /**** Reading Breast cancer dataset from CSV files and copying it to Dataframe ****/
dataframe = pd.read_csv('Downloads/Breast_Cancer_Dataset.csv')

"""
/**** Spliting Dataframe into small Dataset to use it as Trainingdataset to predict from the model ****/
"""
j=389
Training_Set= pd.DataFrame(data=dataframe[j:j+1])
Training_Set1=pd.DataFrame(data=Training_Set)
Test_Set=pd.DataFrame(data=dataframe[j:j+1])
Test_Set1=pd.DataFrame(data=Test_Set)

"""
/**** Removing Patient ID and Cell Type columns from the Training dataset ****/
/**** 
"""
Training_Set = Training_Set.drop(labels='Patient ID',axis=1)
Training_Set = Training_Set.drop(labels='Cell Type',axis=1)
Test_Set = Test_Set.drop(labels='Patient ID',axis=1)
Test_Set = Test_Set.drop(labels='Cell Type',axis=1)


"""
/**** Finding Mean and Standard Deviations of all the attributes ****/
"""
Means_Training_Set = Training_Set.aggregate('mean')
Std_Training_Set = Training_Set.std()

"""
/**** Copy Cell_Type attribute into Cells list and convert Cells list values as 0's and 1's ****/
/**** Here 0 represents cell type Malignant and 1 represents Benign respectively ****/
"""
Cell_Type = {'Cell_Type':['M','B']}
list_Celltype = [0]
list_Celltype1 =[1]
#print(type(list_Celltype))
#print(type(list_Celltype1))

Mal=20
Ben =30


    
Prob_Mal = Mal/(Mal+Ben)
Prob_Ben = Ben/(Mal+Ben)

def Training(j):

  """
  /**** Finding P(x=Radius|C=Malignant)****/
  /**** Finding P(x=Radius|C=Benign) ****/
  /**** Radius contains 10 distinct values with 4 Radius values to satisfy the condition class=malignant mean(Mean_Radius_Mal) and standard deviation(Std_Radius_Mal)
  /**** Consider Gausian Naive Bayes to find the P(X|Ci) ****/
  """
  feature_names = ['Radius','Texture','Perimeter','Area','Smoothness','Compactness','Concavity','Concave points','Symmetry','Fractal dimension','SE_Radius','SE_Texture','SE_Perimeter','SE_Area','SE_Smoothness','SE_Compactness','SE_Concavity','SE_Concave points','SE_Symmetry','SE_Fractal dimension','Worst_Radius','Worst_Texture','Worst_Perimeter','Worst_Area','Worst_Smoothness','Worst_Compactness','Worst_Concavity','Worst_Concave points','Worst_Symmetry','Worst_Fractal dimension']
  #record_number = 45
